{
    "name": "php-soft/json-api-builder",
    "type": "library",
    "description": "This package is auto generate data follow jsonapi.org.",
    "keywords": [
        "PhpSoft",
        "JsonApiBuilder"
    ],
    "homepage": "https://github.com/leeduc/json-api-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Le Duc",
            "email": "ducl@greenglobal.vn",
            "homepage": "http://greenglobal.vn",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.2",
        "php" : "~5.5|~7.0",
        "fzaninotto/faker": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "PhpSoft\\JsonApiBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSoft\\JsonApiBuilder\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
